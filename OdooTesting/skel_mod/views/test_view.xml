<?xml version="1.0" encoding="utf-8"?>
<odoo>
    
    
    <record id="view_my_experience_tree" model="ir.ui.view">
        <field name="name">view_my_experience_tree</field>
        <field name="model">my.experience.module</field>
        <!--<field name="inherit_id" ref="view_hr_payslip_tree_compat" />-->
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
            </tree>
        </field>
    </record>
    
    <record id="view_my_experience_form" model="ir.ui.view">
        <field name="name">view_my_experience_form</field>
        <field name="model">my.experience.module</field>
        <!--<field name="inherit_id" ref="view_hr_payslip_form_compat"/>-->
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" />
                </group>
            </form>
        </field>
    </record>
    
    
    
    

    <record id="action_my_experience" model="ir.actions.act_window">
        <field name="name">Experience Module</field>
        <field name="res_model">my.experience.module</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>
